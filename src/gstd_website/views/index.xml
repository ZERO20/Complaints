<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
        <template id="index_assets" name="index_assets" inherit_id="website.assets_frontend">
            <xpath expr="//link[last()]" position="after">
                <link href="https://fonts.googleapis.com/css?family=Roboto:100,400,300,300italic,400italic,500,700|Indie+Flower:400|"/>
                <link href="/gstd_website/static/assets/css/icons/icomoon/styles.css" rel="stylesheet" type="text/css"/>
                <link href="/gstd_website/static/assets/css/bootstrap.css" rel="stylesheet" type="text/css"/>
                <link href="/gstd_website/static/assets/css/core.css" rel="stylesheet" type="text/css"/>
                <link href="/gstd_website/static/assets/css/components.css" rel="stylesheet" type="text/css"/>
                <link href="/gstd_website/static/assets/css/colors.css" rel="stylesheet" type="text/css"/>
            </xpath>
            <xpath expr="//script[last()]" position="after">
                    <!-- Core JS files -->
                    <script type="text/javascript" src="/gstd_website/static/assets/js/plugins/loaders/pace.min.js"></script>
                    <script type="text/javascript" src="/gstd_website/static/assets/js/core/libraries/jquery.min.js"></script>
                    <script type="text/javascript" src="/gstd_website/static/assets/js/core/libraries/bootstrap.min.js"></script>
                    <script type="text/javascript" src="/gstd_website/static/assets/js/plugins/loaders/blockui.min.js"></script>
                    <script type="text/javascript" src="/gstd_website/static/assets/js/plugins/ui/nicescroll.min.js"></script>
                    <script type="text/javascript" src="/gstd_website/static/assets/js/plugins/ui/drilldown.js"></script>
                    <!-- /core JS files -->

                    <!-- Theme JS files -->
                    <script type="text/javascript" src="/gstd_website/static/assets/js/plugins/visualization/d3/d3.min.js"></script>
                    <script type="text/javascript" src="/gstd_website/static/assets/js/plugins/visualization/d3/d3_tooltip.js"></script>
                    <script type="text/javascript" src="/gstd_website/static/assets/js/plugins/forms/styling/switchery.min.js"></script>
                    <script type="text/javascript" src="/gstd_website/static/assets/js/plugins/forms/styling/uniform.min.js"></script>
                    <script type="text/javascript" src="/gstd_website/static/assets/js/plugins/forms/selects/bootstrap_multiselect.js"></script>
                    <script type="text/javascript" src="/gstd_website/static/assets/js/plugins/ui/moment/moment.min.js"></script>
                    <script type="text/javascript" src="/gstd_website/static/assets/js/plugins/pickers/daterangepicker.js"></script>
                    <script type="text/javascript" src="/gstd_website/static/assets/js/core/app.js"></script>
                    <script type="text/javascript" src="/gstd_website/static/assets/js/pages/dashboard.js"></script>
                    <script type="text/javascript" src="/gstd_website/static/assets/js/plugins/ui/ripple.min.js"></script>
                <!-- /theme JS files -->
            </xpath>
        </template>
		    <template id="theme_epona_layout" inherit_id="website.layout">
			<xpath expr="//div[@id='wrapwrap']/header" position="replace">
				<header>
                    <div class="navbar navbar-inverse bg-indigo">
                        <div class="navbar-header">
                            <a class="navbar-brand" href="index.html"><img src="assets/images/logo_light.png" alt=""/></a>

                            <ul class="nav navbar-nav pull-right visible-xs-block">
                                <li><a data-toggle="collapse" data-target="#navbar-mobile"><i class="icon-tree5"></i></a></li>
                            </ul>
                        </div>
                        <div class="navbar-collapse collapse" id="navbar-mobile">
                            <ul class="nav navbar-nav navbar-right">
                                <li>
                                    <a href="#">
                                        <i class="icon-display4"></i> <span class="visible-xs-inline-block position-right"> Go to website</span>
                                    </a>
                                </li>

                                <li>
                                    <a href="#">
                                        <i class="icon-user-tie"></i> <span class="visible-xs-inline-block position-right"> Contact admin</span>
                                    </a>
                                </li>

                                <li class="dropdown">
                                    <a class="dropdown-toggle" data-toggle="dropdown">
                                        <i class="icon-cog3"></i>
                                        <span class="visible-xs-inline-block position-right"> Options</span>
                                    </a>
                                </li>
                            </ul>
                        </div>
                    </div>
                </header>		
			</xpath>
			<xpath expr="//div[@id='wrapwrap']/main" position="replace">
				<main>
                    <!-- Page container -->
                    <div class="page-container">
                        <!-- Page content -->
                        <div class="page-content">
                            <!-- Main content -->
                            <div class="content-wrapper">
                                <!-- Simple login form -->
                                <form>
                                    <div class="panel panel-body login-form">
                                        <div class="text-center">
                                            <div class="icon-object border-slate-300 text-slate-300"><i class="icon-reading"></i></div>
                                            <h5 class="content-group">Login to your account <small class="display-block">Enter your credentials below</small></h5>
                                        </div>
                                        <div class="form-group has-feedback has-feedback-left">
                                            <input type="text" class="form-control" placeholder="Username"/>
                                            <div class="form-control-feedback">
                                                <i class="icon-user text-muted"></i>
                                            </div>
                                        </div>
                                        <div class="form-group has-feedback has-feedback-left">
                                            <input type="password" class="form-control" placeholder="Password"/>
                                            <div class="form-control-feedback">
                                                <i class="icon-lock2 text-muted"></i>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <button type="submit" class="btn bg-pink-400 btn-block">Sign in <i class="icon-circle-right2 position-right"></i></button>
                                        </div>
                                        <div class="text-center">
                                            <a href="login_password_recover.html">Forgot password?</a>
                                        </div>
                                    </div>
                                </form>
                                <!-- /simple login form -->
                            </div>
                            <!-- /main content -->
                        </div>
                        <!-- /page content -->
                    </div>
                    <!-- /page container -->
                </main>	
			</xpath>
			<xpath expr="//div[@id='wrapwrap']/footer" position="replace">
                <!-- Footer -->
                <div class="footer text-muted text-center">
                   2015. <a href="#">Limitless Web App Kit</a> by <a href="http://themeforest.net/user/Kopyov" target="_blank">Eugene Kopyov</a>
                </div>
                <!-- /footer -->	
			</xpath>
		</template>
	</data>
</odoo>